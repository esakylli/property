{
  "name": "@promaster/api-ordering-code",
  "version": "1.0.0",
  "description": "Promaster SDK Portable Libraries",
  "repository": "https://gitlab.divid.se/promaster-sdk",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "license": "UNLICENSED",
  "author": "Divid Promaster AB",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/api": "^1.0.0",
    "@promaster/uom": "^1.0.0",
    "@promaster/property": "^1.0.0",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.25.29"
  },
  "scripts": {
    "start":
      "webpack-dev-server --config example/webpack.config.js --content-base ./example",
    "lint": "tslint -t msbuild './{src,test}/**/*.ts{,x}'",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --module es2015 --outDir ./lib-esm",
    "test": "rm -rf test/lib && mocha --colors",
    "test:build": "cd test && tsc",
    "example:build": "cd example && tsc"
  }
