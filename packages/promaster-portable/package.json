  "dependencies": {
    "@promaster/promaster-primitives": "^2.3.3",
    "base64-js": "^1.2.0",
    "blob-stream": "^0.1.3",
    "isomorphic-fetch": "^2.2.1",
    "jszip": "^3.1.3",
    "markdown-to-ast": "^3.4.0",
    "ramda": "^0.22.1",
    "showdown": "^1.5.0",
    "ts-exhaustive-check": "^1.0.0"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/blob-stream": "^0.1.29",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jszip": "0.0.31",
    "@types/pdfkit": "^0.7.31",
    "@types/ramda": "types/npm-ramda",
