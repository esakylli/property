  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "license": "UNLICENSED",
  "author": "Divid Promaster AB",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/promaster-primitives": "^3.0.0",
    "@promaster/promaster-markdown": "^1.0.0",
    "@promaster/variant-listing": "^1.0.0",
    "@promaster/api": "^1.0.0",
    "@promaster/api-ordering-code": "^1.0.0",
    "@promaster/api-properties": "^1.0.0",
    "base64-js": "^1.2.0",
    "blob-stream": "^0.1.3",
    "isomorphic-fetch": "^2.2.1",
    "jszip": "^3.1.3",
    "ramda": "^0.22.1",
    "ts-exhaustive-check": "^1.0.0"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/blob-stream": "^0.1.29",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jszip": "0.0.31",
    "@types/pdfkit": "^0.7.31",
    "@types/ramda": "^0.25.29",
    "@types/react": "^15.0.34",
    "@types/react-dom": "^15.5.1",
    "pdfkit": "^0.8.0"
  },
  "scripts": {
    "start":
      "webpack-dev-server --config example/webpack.config.js --content-base ./example",
    "lint": "tslint -t msbuild './{src,test}/**/*.ts{,x}'",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --module es2015 --outDir ./lib-esm",
    "test": "rm -rf test/lib && mocha --colors",
    "test:build": "cd test && tsc",
    "example:build": "cd example && tsc"
  }
}
