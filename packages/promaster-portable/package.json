{
  "name": "@promaster/promaster-portable",
  "version": "0.29.0",
  "description": "Promaster SDK Portable Libraries",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-portable",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "license": "UNLICENSED",
  "author": "Divid Promaster AB",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/promaster-primitives": "^0.13.0",
    "base64-js": "^1.2.0",
    "blob-stream": "^0.1.3",
    "isomorphic-fetch": "^2.2.1",
    "jszip": "^3.1.3",
    "markdown-to-ast": "^3.4.0",
    "node-uuid": "^1.4.7",
    "ramda": "^0.22.1",
    "showdown": "^1.5.0",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/blob-stream": "^0.1.29",
    "@types/chai": "^3.4.34",
    "@types/isomorphic-fetch": "^0.0.33",
    "@types/jszip": "0.0.31",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/node-uuid": "0.0.28",
    "@types/pdfkit": "^0.7.31",
    "@types/ramda": "types/npm-ramda",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/showdown": "^1.4.31",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "chai": "^3.5.0",
    "http-server": "^0.9.0",
    "mocha": "^3.1.2",
    "pdfkit": "^0.8.0",
    "plugin-typescript": "^5.2.7",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "shelljs": "^0.7.4",
    "ts-loader": "^0.9.5",
    "ts-node": "^2.0.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config example/webpack.config.js --content-base ./example",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm",
    "publish:major": "yarn test && yarn build && node scripts/publish.js major",
    "publish:minor": "yarn test && yarn build && node scripts/publish.js minor",
    "publish:patch": "yarn test && yarn build && node scripts/publish.js patch",
    "tsc": "tsc -p src",
    "tsc:test": "tsc -p test",
    "tsc:example": "tsc -p example",
    "tsc:watch": "tsc --watch -p src",
    "tsc:example:watch": "tsc --watch -p example",
    "test": "cd test && mocha --timeout 5000 --require \"ts-node/register\" \"**/*.ts{,x}\""
  }
}