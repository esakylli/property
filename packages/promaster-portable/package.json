{
  "name": "@promaster/promaster-portable",
  "version": "0.7.0",
  "description": "Promaster SDK Portable Libraries",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-portable",
  "main": "lib/index.js",
  "typings": "lib/index",
  "license": "UNLICENSED",
  "author": "Divid Promaster AB",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "scripts": {
    "start": "http-server -c-1",
    "build": "rm -rf lib && tsc --version && tsc -p src",
    "publish:major": "npm run build && node scripts/publish.js major",
    "publish:minor": "npm run build && node scripts/publish.js minor",
    "publish:patch": "npm run build && node scripts/publish.js patch",
    "tsc": "tsc -p src",
    "tsc:watch": "tsc --watch -p src",
    "tsc:example": "tsc -p example",
    "tsc:example:watch": "tsc --watch -p example",
    "test": "mocha --require ts-node/register test/**/*.ts"
  },
  "dependencies": {
    "node-uuid": "^1.4.7",
    "@promaster/promaster-primitives": "^0.3.6",
    "showdown": "^1.5.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/node-uuid": "0.0.28",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/showdown": "^1.4.31",
    "chai": "^3.5.0",
    "http-server": "^0.9.0",
    "mocha": "^3.1.2",
    "plugin-typescript": "^5.2.7",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "shelljs": "^0.7.4",
    "ts-loader": "^0.9.5",
    "ts-node": "^1.6.0",
    "typescript": "^2.0.3"
  }
}