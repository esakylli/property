{
  "name": "@promaster/react-property-selectors",
  "version": "1.0.2",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "description": "React components for applications that use Promaster.",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/property": "^1.0.4",
    "@promaster/property-filtering": "^1.0.4",
    "react": "^15.6.1",
    "uom": "^1.0.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "@types/react": "^15.6.0"
  },
  "scripts": {
    "start": "yarn run storybook",
    "lint": "tslint -t msbuild './{src,test}/**/*.ts{,x}'",
    "test": "jest --config=test/jest.config.json --colors",
    "circular": "madge --circular ./src/index.tsx",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm",
    "tsc:test": "tsc -p test --noEmit",
    "tsc:stories": "tsc -p stories --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
