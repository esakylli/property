{
  "name": "@promaster/react-properties-selector",
  "version": "2.0.0",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "description": "User interface for selecting a valid property value set",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/packages/react-properties-selector",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/property": "^1.1.3",
    "@promaster/property-filter-pretty": "^1.1.1",
    "@promaster/react-property-selectors": "^2.0.0",
    "ramda": "^0.25.0",
    "react": "^16.6.1",
    "uom": "^1.1.3"
  },
  "devDependencies": {
    "@types/ramda": "^0.25.29",
    "@types/react": "^16.7.3",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.0.3",
    "csjs": "1.0.6",
    "insert-css": "^2.0.0",
    "styled-components": "^4.0.3"
  },
  "scripts": {
    "start": "yarn run storybook",
    "lint": "tslint -t msbuild './{src,test}/**/*.ts{,x}'",
    "test": "jest --config=test/jest.config.json --colors",
    "circular": "madge --circular ./src/index.tsx",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm",
    "tsc:test": "tsc -p test --noEmit",
    "tsc:stories": "tsc -p stories --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
