{
  "version": "0.1.0",
  "tasks": [
    {
      "taskName": "tsc",
      "command": "node",
      "args": [
        "./yarn.js",
        "tsc"
      ],
      "isShellCommand": true,
      "isBackground": true,
      "isBuildCommand": true,
      "problemMatcher": "$tsc"
    },
    {
      "taskName": "tsc:watch",
      "command": "yarn",
      "isShellCommand": true,
      "isBackground": true,
      "args": [
        "tsc:watch"
      ],
      "showOutput": "always",
      "problemMatcher": "$tsc-watch"
    },
    {
      "taskName": "tslint",
      "command": "node",
      "isShellCommand": true,
      "showOutput": "always",
      "problemMatcher": {
        "base": "$tslint5",
        "fileLocation": "relative"
      },
      "args": [
        "yarn",
        "lint"
      ]
    }
  ]
}