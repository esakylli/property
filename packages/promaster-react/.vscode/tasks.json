{
  "version": "0.1.0",
  "tasks": [
    {
      "taskName": "tsc",
      "command": "node",
      "args": [
        "./yarn.js",
        "tsc"
      ],
      "isShellCommand": true,
      "isBackground": true,
      "isBuildCommand": true,
      "problemMatcher": "$tsc"
    },
    {
      "taskName": "lint",
      "command": "node",
      "args": [
        "./yarn.js",
        "lint"
      ],
      "isShellCommand": true,
      "problemMatcher": {
        "owner": "tslint",
        "fileLocation": "relative",
        "severity": "warning",
        "pattern": {
          "regexp": "^(\\S.*)\\[(\\d+), (\\d+)\\]:\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      }
    }
  ]
}
