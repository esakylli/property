{
  "name": "@promaster/promaster-react",
  "version": "0.28.0",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "description": "React components for applications that use Promaster.",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-react",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/promaster-portable": "^0.32.0",
    "@promaster/promaster-primitives": "^0.15.0",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.8",
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.9",
    "@types/ramda": "^0.0.3",
    "@types/react": "^15.0.26",
    "@types/react-dom": "^0.14.18",
    "@types/react-test-renderer": "^15.5.0",
    "awesome-typescript-loader": "^3.1.3",
    "csjs": "1.0.6",
    "http-server": "^0.9.0",
    "insert-css": "^2.0.0",
    "jest": "^20.0.4",
    "madge": "^1.6.0",
    "plugin-typescript": "^5.2.7",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.4.2",
    "shelljs": "^0.7.4",
    "styled-components": "^2.1.0",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.2.2",
    "tslint": "^5.4.2",
    "tslint-immutable": "^2.1.0",
    "typescript": "^2.4.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "scripts": {
    "start": "node ./yarn storybook",
    "tsc": "tsc -p src --noEmit",
    "tsc:test": "tsc -p test --noEmit",
    "tsc:stories": "tsc -p stories --noEmit",
    "test": "jest --config=test/jest.config.json",
    "lint": "tslint --project src/tsconfig.json --type-check './src/**/*.ts{,x}'",
    "circular": "madge --circular ./src/index.tsx",
    "build": "node ./yarn.js build:clean && node ./yarn.js build:cjs && node ./yarn.js build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm",
    "publish:major": "yarn build && node scripts/publish.js major",
    "publish:minor": "yarn build && node scripts/publish.js minor",
    "publish:patch": "yarn build && node scripts/publish.js patch",
    "promaster-upgrade": "yarn upgrade @promaster/promaster-primitives && yarn upgrade @promaster/promaster-portable",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}