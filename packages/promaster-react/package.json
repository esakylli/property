{
  "name": "@promaster/promaster-react",
  "version": "1.1.0",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "description": "React components for applications that use Promaster.",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-react",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "dependencies": {
    "@promaster/promaster-portable": "^1.1.0",
    "@promaster/promaster-primitives": "^3.0.0",
    "@types/ramda": "^0.25.17",
    "ramda": "^0.25.0",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.12",
    "@types/react": "^15.6.0",
    "@types/react-test-renderer": "^15.5.0",
    "babel-core": "^6.26.0",
    "csjs": "1.0.6",
    "insert-css": "^2.0.0",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "styled-components": "^3.1.6"
  },
  "scripts": {
    "start": "yarn run storybook",
    "lint": "tslint -t msbuild './{src,test}/**/*.ts{,x}'",
    "test": "jest --config=test/jest.config.json --colors",
    "circular": "madge --circular ./src/index.tsx",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm",
    "tsc:test": "tsc -p test --noEmit",
    "tsc:stories": "tsc -p stories --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
