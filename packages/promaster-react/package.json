{
  "name": "promaster-react",
  "version": "0.5.1",
  "main": "lib/index.js",
  "description": "React components for applications that use Promaster.",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-react",
  "typings": "lib/index",
  "license": "UNLICENSED",
  "scripts": {
    "start": "http-server -c-1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc --version && tsc -p src",
    "tsc:example": "tsc -p example",
    "build": "rm -rf lib && tsc --version && tsc -p src",
    "publish:major": "npm run build && node scripts/publish.js major",
    "publish:minor": "npm run build && node scripts/publish.js minor",
    "publish:patch": "npm run build && node scripts/publish.js patch"
  },
  "dependencies": {
    "csjs": "^1.0.6",
    "insert-css": "^1.0.0",
    "promaster-portable": "git+https://pm-sdk:pm-sdk-read@gitlab.divid.se/promaster-sdk/promaster-portable.git#0.4.2",
    "promaster-primitives": "git+https://pm-sdk:pm-sdk-read@gitlab.divid.se/promaster-sdk/promaster-primitives.git#0.3.0",
    "react": "^15.3.2"
  },
  "devDependencies": {
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "http-server": "^0.9.0",
    "plugin-typescript": "^5.2.7",
    "shelljs": "^0.7.4",
    "typescript": "^2.0.3",
    "react-dom": "^15.3.2"
  },
  "jspm": {
    "dependencies": {
      "csjs": "npm:csjs^1.0.6",
      "promaster-primitives": "divid-gitlab:promaster-sdk/promaster-primitives@^0.3.0",
      "promaster-portable": "divid-gitlab:promaster-sdk/promaster-portable@^0.4.2",
      "react": "npm:react@^15.3.2"
    }
  }
}