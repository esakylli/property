{
  "name": "@promaster/promaster-primitives",
  "version": "0.3.12",
  "main": "lib/index.js",
  "description": "Promster SDK Primitives",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-primitives",
  "typings": "lib/index",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "scripts": {
    "build": "rm -rf lib && tsc --version && tsc -p src",
    "publish:major": "npm run build && node scripts/publish.js major",
    "publish:minor": "npm run build && node scripts/publish.js minor",
    "publish:patch": "npm run build && node scripts/publish.js patch",
    "tsc": "tsc -p src",
    "tsc:test": "tsc",
    "gen-doc": "npm run build && node scripts/gen-doc.js",
    "gen-typedoc": "typedoc --hideGenerator --theme node_modules/typedoc-markdown-theme/bin --out ./doc/typedoc ./src",
    "test": "mocha --compilers ts:ts-node/register test/**/*.ts",
    "test:measure": "mocha --compilers ts:ts-node/register test/measure/*.ts",
    "test:properties": "mocha --compilers ts:ts-node/register test/product-properties/*.ts",
    "test:filtering": "mocha --compilers ts:ts-node/register test/property-filtering/property-filter-test.ts",
    "test:parser": "mocha --compilers ts:ts-node/register test/property-filtering/pegjs/property-filter-test.ts",
    "gen_parser": "pegjs ./src/property-filtering/pegjs/parser.pegjs ./src/property-filtering/pegjs/parser.ts",
    "benchmark": "node ./lib/test/property_filtering/property_filter_benchmark.js"
  },
  "devDependencies": {
    "@types/assertion-error": "^1.0.30",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/pegjs": "0.0.28",
    "chai": "^3.5.0",
    "jsdoc-to-markdown": "^2.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "pegjs": "0.9.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "ts-node": "^1.6.0",
    "typedoc": "^0.5.1",
    "typedoc-markdown-theme": "0.0.4",
    "typescript": "^2.0.10"
  }
}