{
  "name": "@promaster/promaster-primitives",
  "version": "0.6.1",
  "main": "lib-cjs/index.js",
  "module": "lib-esm/index.js",
  "types": "lib-cjs/index",
  "description": "Promster SDK Primitives",
  "author": "Divid AB",
  "repository": "https://gitlab.divid.se/promaster-sdk/promaster-primitives",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.divid.se"
  },
  "devDependencies": {
    "@types/assertion-error": "^1.0.30",
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/pegjs": "0.0.28",
    "chai": "^3.5.0",
    "jsdoc-to-markdown": "^2.0.1",
    "madge": "^1.6.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "pegjs": "0.9.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "ts-node": "^2.0.0",
    "tslint": "^4.5.1",
    "tslint-immutable": "^2.1.0",
    "typedoc": "^0.5.1",
    "typedoc-markdown-theme": "0.0.4",
    "typescript": "^2.2.1"
  },
  "scripts": {
    "lint": "tslint './src/**/*.ts{,x}'",
    "circular": "madge --circular ./src/index.ts",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm",
    "build:clean": "rm -rf ./lib-cjs ./lib-esm",
    "build:cjs": "tsc -p src && yarn build:parser:cjs",
    "build:esm": "tsc -p src --module es2015 --outDir ./lib-esm && yarn build:parser:esm",
    "build:parser:cjs": "cp src/property-filtering/pegjs/generated-parser.* lib-cjs/property-filtering/pegjs/",
    "build:parser:esm": "cp src/property-filtering/pegjs/generated-parser.* lib-esm/property-filtering/pegjs/",
    "publish:major": "yarn test && yarn build && node scripts/publish.js major",
    "publish:minor": "yarn test && yarn build && node scripts/publish.js minor",
    "publish:patch": "yarn test && yarn build && node scripts/publish.js patch",
    "tsc": "tsc -p src",
    "tsc:test": "tsc -p test",
    "gen-doc": "yarn build && node scripts/gen-doc.js",
    "gen-typedoc": "typedoc --hideGenerator --theme node_modules/typedoc-markdown-theme/bin --out ./doc/typedoc ./src",
    "test": "mocha --compilers ts:ts-node/register test/**/*.ts",
    "test:measure": "mocha --compilers ts:ts-node/register test/measure/*.ts",
    "test:properties": "mocha --compilers ts:ts-node/register test/product-properties/*.ts",
    "test:filtering": "mocha --compilers ts:ts-node/register test/property-filtering/property-filter-test.ts",
    "test:parser": "mocha --compilers ts:ts-node/register test/property-filtering/pegjs/property-filter-test.ts",
    "generate-parser": "pegjs ./src/property-filtering/pegjs/parser.pegjs ./src/property-filtering/pegjs/generated-parser.js"
  }
}
